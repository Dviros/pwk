#-- VPN --#
# Print my pass for CSJ vpn
alias vp='cat /home/kali/CSJ/vpnPassChristine'
# Print VPN password, then log into the CSJ lab
alias v='vp; cd /home/kali/CSJ/vpnChristine && sudo openvpn --config CSJ-PfSense-01-UDP4-5050-Christine-config.ovpn' 

#-- CHANGE DIRECTORIES --#
# Go to my PWK repo
alias p='cd /home/kali/gitWorkspace/pwk'
# Change to the parent directory
alias ..='cd ../'
# Change to mintShared directory
alias m='cd /home/kali/mint'

#-- OS COMMANDS --#
# Print out bashrc aliases
alias a='cat $HOME/.bash_aliases | grep "^alias " | cut -f1 -d" " --complement'
# Reboot
alias r='sudo shutdown -r now'
# Shutdown
alias s='sudo shutdown -h now'
